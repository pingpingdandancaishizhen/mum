									                                                <c:forEach items="${product.cydfeeConfig.monthlyFee}" var="day">
					                                                
					                                                <tr id="fmt_day_${day.repaymentType }"><td>还款方式：<span>${day.repaymentTypeName}</span></td><td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td><td>综合日利率：<input   type="text" value="${day.start}">-<input   type="text" value="${day.end}">%</td></tr>    
					                                                </c:forEach>
				                                                </c:if>          
				                                                
				                                     </table>                                 		                     
					                                    
				                                    </div>
				                                    
				                                </div>
				                            </div>
