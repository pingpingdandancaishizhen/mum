@charset "utf-8";
/*全局样式，应该到所有共有部分（如登录、注册、页头、页尾），谨慎修改*/
html,
body,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
p,
iframe,
dl,
dt,
dd,
ul,
ol,
li,
pre,
form,
fieldset,
button,
input,
textarea,
th,
td {
  margin: 0;
  padding: 0;
}
body,
button,
input,
select,
textarea,
fieldset {
  font: 14px/1.5 arial, "Microsoft Yahei", "Hiragino Sans GB", sans-serif;
  color: #666;
  background: #f1f1f1;
}
