.plugin-position ul,
.plugin-position ol,
.plugin-position li {
  list-style: none;
  list-style-type: none;
}
.plugin-position fieldset,
.plugin-position img {
  border: 0 none;
}
.plugin-position img {
  -ms-interpolation-mode: bicubic;
}
.plugin-position table {
  border-collapse: collapse;
  border-spacing: 0;
}
.plugin-position div,
.plugin-position dl,
.plugin-position dt,
.plugin-position dd,
.plugin-position ul,
.plugin-position ol,
.plugin-position li,
.plugin-position a {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.plugin-position input,
.plugin-position select,
.plugin-position textarea,
.plugin-position button {
  font-size: 100%;
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
  vertical-align: middle;
}
.plugin-position button,
.plugin-position input[type=button],
.plugin-position input[type=submit] {
  cursor: pointer;
}
.plugin-position input[type=search] {
  -webkit-appearance: textfield;
}
.plugin-position textarea {
  overflow-y: auto;
  resize: vertical;
}
.plugin-position h1 {
  font-size: 36px;
}
.plugin-position h2 {
  font-size: 28px;
}
.plugin-position h3 {
  font-size: 24px;
}
.plugin-position h4 {
  font-size: 22px;
}
.plugin-position h5 {
  font-size: 18px;
}
.plugin-position h6 {
  font-size: 16px;
}
.plugin-position h1,
.plugin-position h2,
.plugin-position h3,
.plugin-position h4,
.plugin-position h5,
.plugin-position h6 {
  color: #333333;
  line-height: 1.8em;
  letter-spacing: 0.1em;
}
.plugin-position a {
  color: #333;
  text-decoration: none;
}
.plugin-position a:hover {
  color: #000;
}
.plugin-position input:focus {
  box-shadow: 0px 0px 10px #8dc437;
}
.plugin-position .reg_error {
  /*边框提示*/
  border: 1px solid #f2766a !important;
}
.plugin-position .reg_txt .reg_err {
  /*文字提示 在html 的类中加reg_err*/
  display: none;
}
.plugin-position .floatl {
  float: left;
}
.plugin-position .floatr {
  float: right;
}
.plugin-position .clearb {
  clear: both;
}
.plugin-position .mask-Bg {
  background: #000;
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: 9999;
  display: none;
  opacity: 0.5;
}
.plugin-position .layer-LogReg {
  visibility: hidden;
  top: 50%;
  left: 50%;
  position: absolute;
  z-index: 10000;
  height: 592px;
  width: 612px;
  margin: -290px 0 0 -290px;
  background: #fff;
  padding: 10px;
  box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.3);
  border-radius: 12px;
}
.plugin-position .close {
  font-size: 40px;
  line-height: .5;
  position: absolute;
  top: 20px;
  right: 20px;
  color: #aaa;
  text-shadow: 0 -1px 1px rbga(0, 0, 0, 0.6);
  cursor: pointer;
}
.plugin-position .container {
  width: 60%;
  margin: 2% 20%;
  color: #333;
}
.plugin-position .layer-LogReg h5 {
  text-align: center;
  font-size: 24px;
  color: #333;
}
.plugin-position .loginH5 {
  padding-top: 100px !important;
  padding-bottom: 24px;
}
.plugin-position .xuanze {
  margin-top: 10px;
  border-radius: 4px;
}
.plugin-position .xuanze a {
  color: #f2766a;
}
.plugin-position .xuanze a:hover {
  color: #f79b92;
}
.plugin-position .loginbutton {
  margin-top: 10px;
}
.plugin-position .loginbutton a {
  padding: 10px;
  display: block;
  color: #fff;
  font-size: 18px;
  text-align: center;
  border-radius: 4px;
  background-color: #f2766a;
}
.plugin-position .loginbutton a:hover {
  background-color: #e05447;
}
.plugin-position .gologin {
  text-align: center;
  padding-top: 10px;
}
.plugin-position .gologin a {
  font-size: 16px;
  color: #f2766a;
  padding-right: 20px;
  background-image: url(../images/jiantou.png);
  background-repeat: no-repeat;
  background-position: 100% 50%;
}
.plugin-position .reg_txt .reg_pw p {
  padding-left: 18px;
  font-size: 12px;
  margin-top: 5px;
  color: #F2766A;
  background-image: url(../images/warning.png);
  background-repeat: no-repeat;
  background-position: 0 50%;
}
.plugin-position .autologin > span {
  display: block;
  padding-left: 24px;
  line-height: 24px;
  background-image: url(../images/select111.png);
  background-repeat: no-repeat;
  background-position: 0 50%;
  cursor: pointer;
}
.plugin-position .waybtn {
  display: inline-table;
  width: 100%;
  margin-top: 16px;
}
.plugin-position .waybtn li {
  float: left;
  display: inline-block;
  width: 50%;
  height: 38px;
  font-size: 14px;
  line-height: 36px;
  color: #666;
  text-align: center;
  background-color: #f5f5f5;
  cursor: pointer;
}
.plugin-position .waybtn .activeway {
  height: 46px;
  background-color: #fff;
  background-image: url(../images/waybj.png);
  background-repeat: no-repeat;
  background-position: 50% 50%;
  color: #fff;
}
.plugin-position .myForm1 {
  display: block;
}
.plugin-position .myForm0 {
  display: none;
}
.plugin-position #zhucefangshi {
  width: 100%;
}
.plugin-position #zhucefangshi dd {
  float: left;
  width: 50%;
  padding: 10px 0;
  padding-left: 26px;
  background-repeat: no-repeat;
  background-position: 0 0px;
  cursor: pointer;
}
.plugin-position #zhucefangshi dd:last-child {
  background-image: url(../images/EmailBJ.jpg);
}
.plugin-position #zhucefangshi dd:first-child {
  background-image: url(../images/phonoBJ.jpg);
}
.plugin-position .activefang {
  color: #ff3333;
  border-bottom: 1px solid #ff3333;
  background-position: 0 -40px !important;
}
.plugin-position .reg_txt {
  position: relative;
  border: 1px solid #e5e5e5;
  zoom: 1;
  height: 43px;
  width: 100%;
  margin-bottom: 24px;
  border-radius: 4px;
  vertical-align: baseline;
  display: inline-block;
  *display: inline;
  z-index: 2;
}
.plugin-position .reg_txt input {
  height: 39px;
  border: 0;
  padding-top: 2px;
  text-indent: 10px;
  line-height: 28px;
  width: 100%;
  border-radius: 3px;
  color: #444;
  background: #fff;
}
.plugin-position .password_box {
  position: absolute;
  color: #949494;
  right: 11px;
  top: 10px;
  font-size: 12px;
  line-height: 12px;
}
.plugin-position .password_box i {
  background: #c5c5c5;
  overflow: hidden;
  display: inline-block;
  width: 14px;
  height: 4px;
  margin-right: 3px;
  margin-top: 3px;
}
.plugin-position .getcode {
  display: inline-block;
  position: absolute;
  top: 5px;
  right: 10px;
  font-size: 12px;
  width: 24%;
  line-height: 30px;
  text-align: center;
  border-radius: 4px;
  color: #fff;
  background-color: #ccc;
}
.plugin-position .getcode a {
  display: block;
  background: #f2766a;
  border-radius: 4px;
  color: #fff;
}
.plugin-position .getcode a:hover {
  background: #e05447;
}
.plugin-position .yzm {
  position: absolute;
  top: 5px;
  right: 40px;
}
.plugin-position img.yzm {
  width: 80px;
  height: 30px;
}
.plugin-position .refreshYzm {
  display: inline-block;
  position: absolute;
  top: 9px;
  right: 9px;
  width: 22px;
  height: 22px;
  background-image: url(../images/refresh.jpg);
  background-repeat: no-repeat;
  background-position: 100% 50%;
  cursor: pointer;
}
.plugin-position .protocol {
  margin-top: 5px;
}
.plugin-position .protocol strong {
  color: #f79b92;
}
.plugin-position .head-all-box {
  width: 100%;
  height: 33px;
  font-size: 12px;
  background: #fff;
  border-bottom: #ccc 1px dotted;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.03);
}
.plugin-position .head-all-contents {
  width: 1190px;
  height: 32px;
  overflow: hidden;
  background: #fff;
  margin: 0 auto;
}
.plugin-position .head-all-contents .nav-l {
  float: left;
  width: 30%;
}
.plugin-position .head-all-contents .nav-l li {
  float: left;
}
.plugin-position .head-all-contents .nav-r {
  width: 50%;
  float: right;
  text-align: right;
}
.plugin-position .head-all-contents .nav-r li {
  float: right;
  color: #333;
  line-height: 32px;
  display: block;
  text-align: left;
  border-left: #eee 1px dotted;
}
.plugin-position .head-all-contents .nav-l a {
  padding: 0 15px;
  line-height: 32px;
  display: block;
}
.plugin-position .head-all-contents .nav-r a {
  padding: 0 15px;
  line-height: 32px;
  display: block;
}
.plugin-position .head-all-contents .nav-r a:hover,
.plugin-position .head-all-contents .nav-l a:hover {
  color: #e05447;
}
.plugin-position .head-all-contents .nav-r li.menu {
  padding: 0 20px 0 15px;
  cursor: pointer;
  background: url(../images/ico-jj1.png) no-repeat 120px 14px;
}
.plugin-position .head-all-contents .nav-r li.menu:hover {
  color: #e05447;
  background: #fff url(../images/ico-jj1.png) no-repeat 120px -28px;
}
.plugin-position .head-all-contents .nav-r li.menu img {
  width: 20px;
  height: 20px;
  display: block;
  float: left;
  margin: 6px 5px 0 0;
}
.plugin-position .head-all-contents .nav-r li.menu .name {
  width: 80px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  display: block;
  float: left;
}
.plugin-position .head-all-contents .nav-r li.menu .sub-menu {
  width: 140px;
  background: #fff;
  position: absolute;
  box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.2);
  display: none;
  margin: 32px 0 0 -15px;
}
.plugin-position .head-all-contents .nav-r li.menu .sub-menu a {
  line-height: 30px;
  width: 100%;
  color: #333;
  border-top: #f1f1f1 1px dotted;
}
.plugin-position .head-all-contents .nav-r li.menu .sub-menu a:hover {
  background: #f7f7f7;
  color: #e05447;
}
.plugin-position .head-all-contents .nav-r li.menu:hover .sub-menu {
  display: block;
}
.plugin-position .head-all-contents .nav-l li.home a {
  background: url(../images/ico-ht.png) no-repeat 10px -26px;
  text-indent: 10px;
}
.plugin-position .head-all-contents .nav-l li.app a {
  background: url(../images/ico-ht.png) no-repeat 10px 11px;
  text-indent: 10px;
}
.plugin-position .head-all-contents .nav-r li .new {
  background: #e05447;
  width: 16px;
  height: 16px;
  font-size: 10px !important;
  font-weight: bold;
  text-align: center;
  line-height: 16px;
  border-radius: 50%;
  margin: 9px 0 0 52px;
  position: absolute;
  color: #fff;
}
.plugin-position .head-all-contents .nav-r li.sms a {
  padding-right: 30px;
}
