.plugin-position .foot-box {
  width: 100%;
  background: #2d2e30;
  margin-top: 20px;
}
.plugin-position .foot-contents {
  width: 1190px;
  background: #2d2e30;
  margin: 0 auto;
  padding: 25px 0 15px 0;
