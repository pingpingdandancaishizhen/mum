<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-4.0.xsd
			http://code.alibabatech.com/schema/dubbo
        	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<context:property-placeholder location="classpath:conf/remote.properties"
		ignore-unresolvable="true" />

	<dubbo:application name="sunfit-p2p-server" />
	<!-- <dubbo:registry address="N/A" /> -->
 
    <dubbo:registry address="${dubbo.transport.registry}"  protocol="zookeeper" file=".dubbo/dubbo-reg.cache"/>
    <dubbo:protocol port="${dubbo.transport.port}" accepts="100" />
    <dubbo:provider version="${dubbo.transport.version}" loadbalance="leastactive" delay="-1" timeout="10000" cluster="failfast" retries="0" />

	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.CorpUserService"	ref="risk.corpUserService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.CorpService"	ref="risk.corpService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.CorpRoleService"	ref="risk.corpRoleService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.CorpDataRoleService"	ref="risk.corpDataRoleService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.system.PermissionService"	ref="risk.permissionService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.system.SystemMenuService"	ref="risk.systemMenuService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.CorpGroupService"	ref="risk.corpGroupService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.CorpDeptService"	ref="risk.corpDeptService" />
		<dubbo:service interface="cn.sunfit.risk.buz.api.service.system.DataService"	ref="risk.dataService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.system.DistrictService"	ref="risk.districtService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.system.CarInfoService"	ref="risk.carInfoService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.ArticleService"	ref="risk.articleService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.CalendarEventService"	ref="risk.calendarEventService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.corp.ResourcesService"	ref="risk.resourcesService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.OSSService"	ref="risk.ossService" />
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.product.P2PProductService" ref="risk.p2PProductService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.excel.OperationLogService" ref="risk.operationLogService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.partnerCorp.P2PAssetService" ref="risk.p2PAssetService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.productType.ProductTypeService" ref="risk.p2pProductTypeService"/>
	
	

    <dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.activiti.SolutionService" ref="risk.p2p.solutionService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.activiti.AssignService" ref="risk.p2p.assignService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.activiti.OperLogService" ref="risk.p2p.operLogService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.activiti.SuperFormService" ref="risk.p2p.superFormService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.excel.P2PBankService" ref="risk.p2PBankService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.activiti.BpService" ref="risk.p2p.bpService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.imp.ImportOrderService" ref="risk.p2p.importOrderService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.order.OrderService" ref="risk.p2p.orderService"/>
	<dubbo:service interface="cn.sunfit.risk.buz.api.service.p2p.productSubType.ProductSubTypeService" ref="risk.p2pProductSubTypeService"/>
	
</beans>
