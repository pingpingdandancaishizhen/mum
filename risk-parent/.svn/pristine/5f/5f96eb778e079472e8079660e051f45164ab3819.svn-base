//自定义checkbox
.checkbox-custom{
  cursor: pointer;
  input{
    width: 17px !important;
    height:17px !important;
    display: inline-block !important;
    padding:0 !important;
    margin:0 !important;
    vertical-align: middle;
    visibility: hidden;
  }
  .checkbox-icon{
    width: 17px;
    height:17px;
    background: url("../../../images/checkbox-icon.png");
    position: absolute;
    top:0;
    bottom: 0;
    margin:auto;
  }
  input[type="checkbox"]+.checkbox-icon{
    background-position: 0 0;
  }
  input[type="checkbox"]:checked+.checkbox-icon{
    background-position: -68px 0;
  }
  &:hover{
    input[type="checkbox"]+.checkbox-icon{
      background-position: -34px 0;
    }
    input[type="checkbox"]:checked+.checkbox-icon{
      background-position: -68px 0;
    }
  }
}
.login-checkbox-custom{
  cursor: pointer;
  input{
    width: 14px !important;
    height:14px !important;
    display: inline-block !important;
    padding:0 !important;
    margin:0 !important;
    vertical-align: middle;
    visibility: hidden;
  }
  .checkbox-icon{
    width: 14px;
    height:14px;
    background: url("../../../images/login-checkbox-icon.png");
    position: absolute;
    top:0;
    bottom: 0;
    margin:auto;
  }
  input[type="checkbox"]+.checkbox-icon{
    background-position: -6px -2px;
  }
  input[type="checkbox"]:checked+.checkbox-icon{
    background-position: -66px -2px;
  }
  &:hover{
    input[type="checkbox"]+.checkbox-icon{
      background-position: -36px -2px;
    }
    input[type="checkbox"]:checked+.checkbox-icon{
      background-position: -66px -2px;
    }
  }
}
