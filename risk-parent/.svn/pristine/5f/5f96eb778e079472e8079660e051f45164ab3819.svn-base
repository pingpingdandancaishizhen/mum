html,
body {
  height: 100%
}
*{
  margin:0;
  padding:0;
}
ul,ol,li{
  list-style: none;
}
label{
  font-weight: 500;
}
body {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-family: '微软雅黑','Source Sans Pro', 'Helvetica Neue', Helvetica, Arial, sans-serif;
  font-weight: 400;
  overflow-x: hidden;
  overflow-y: auto;
}
a,a:hover,a:active{
  outline: none;
  text-decoration: none;
}
.no-padding{
  padding:0 !important;
}
.no-margin{
  margin:0 !important;
}
/* Layout */
.wrapper {
  height: 100%;
  position: relative;
  overflow: hidden;
}
.wrapper:before,
.wrapper:after {
  content: " ";
  display: table;
}
.wrapper:after {
  clear: both;
}
.content-wrapper,.pages-form{
  background: #f7f8fd;
}
.content-wrapper{
  //overflow: auto;
  //height:auto;
}
.content{
  padding:0;
  overflow: auto;
  //overflow: hidden;
}
//nav Tabs
.nav-tabs-custom{
  .nav-tabs{
    background: #FFF;
    height:58px;
    padding-top:18px;
    border-bottom: 1px solid @color-border;
    &>li{
      margin-bottom:0;
      height:100%;
      &:first-child{
        margin-left:5px;
      }
      a{
        border: none;
        color: #838a9c;
        border-radius: inherit;
        line-height: 40px;
        font-size:16px;
        position: relative;
        padding: 0;
        margin:0 20px;
        &:hover,&:focus,&:active{
          color:@color-blue;
          background: transparent;
          &:after{
            content: '';
            height:4px;
            background: @color-blue;
            position: absolute;
            bottom: 0;
            width: 100%;
            display: block;
          }
        }
      }
      &.active{
        a,a:focus,a:hover{
          color:@color-blue;
          background: transparent;
          border: none;
          &:after{
            content: '';
            height:4px;
            background: @color-blue;
            position: absolute;
            bottom: 0;
            width: 100%;
            display: block;
          }
        }
      }
    }
  }
}
.box-body{
  padding:10px 0;
}
.content-padding{
  padding:0 20px;
  //overflow: hidden;
  height:100%;
}
.table-box{
  margin:24px 0;
}
.new-box{
  margin-top:30px;
}
.table-head{
  margin:0 0 15px 0;
  .table-head-title{
    font-size:16px;
    color:#333333;
  }
  span{
    display: inline-block;
  }
}
.box{
  border:1px solid @color-grey;
}
.no-margin{
  margin:0;
}

//table 表单
.bootstrap-table{
  .fixed-table-container{
    border-radius: 0;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border:none;
    &>div{
      //overflow: hidden;
      &:after{
        content: '';
        display: block;
        clear: both;
      }
    }
  }
  .fixed-table-body,.fixed-table-toolbar{
    border:1px solid @color-border;
  }
  .fixed-table-toolbar{
    background: @color-grey;
    .bs-bars{
      line-height: 1;
    }
  }
  .fixed-table-toolbar{
    margin-top:-2px;
    padding:0 8px;
    .bs-bars{
      margin:8px 0;
    }
    .btn+.btn{
      margin-left:15px;
    }
  }
  .table{
    font-size:13px;
    //min-width: 1200px;
    background: #fff;
    border-bottom: none;
    thead{
      height:50px;
      background: @color-table-thread;
    }
    tr{
      //border-bottom: 1px solid @color-border;
      &:last-child{
        border-bottom: none;
      }
      td,th{
        border: none;
        padding:0 20px !important;
        font-weight: 500;
        height:50px;
        vertical-align: middle !important;
        &.table-no-padding{
          padding:0 !important;
        }
        &>div{
          vertical-align: middle;
        }
        .table-bordered{
          margin:-1px 0;
          border-bottom:0;
          td{
            padding-left:5px;
            padding-right:5px;
          }
        }


      }
      .th-inner{
        padding:0;
        line-height: 1;
      }
    }
    tbody{

      .treenode{
        .vertical, .vertical.last{
          border-left: solid #a0a3ac 2px;
          height: 16px;
          margin: 7.5px 0 7.5px 4px;
        }
        .vertical.last{
          height: 9px;
        }
        .node{
          border-top: solid #a0a3ac 2px;
          width: 9px;
          margin-right:4px;
        }

      }
    }
    &.table-hover>tbody>tr:not(.no-records-found){
      &:hover,&.selected>td{
        background: @color-table-hover;
        border-bottom: 1px solid @color-table-hover-border;
        border-top: 1px solid @color-table-hover-border;
      }
      &:hover{
        //box-shadow: 0 0 7px 1px @color-table-hover;
      }
    }
  }
  .table-responsive{
    width: 100% ;
    min-width: 100%;
  }
  .table-striped tbody{
    tr{
      &:nth-child(2n-1){
        background: #fff;
      }
      &:nth-child(2n){
        background: @color-grey;
      }
    }
  }

  .no-table-bg>tbody{
    &>tr:nth-child(2n-1){
      background: #fff;
    }
    &>tr:nth-child(2n){
      background: #fff;
    }

  }
}
td >table{
  width: 100%;
}
td >.table-bordered{
  //border-right:none !important;
}
.inner-form{
  height: 100%;
  width: 100%;
  //border-left:1px solid @color-border;
  //border-right:1px solid @color-border;
  display: inline-table;
  span{
    width: 100%;
    padding:15px;
    display: inline-block;
    &:last-child{
      border-bottom:none;
    }
    &:nth-child(2n){
      background: @color-body-bg;
    }
  }
  span+span{
    border-top:1px solid @color-border;
  }
}
.table-top-tool,.table-bottom-tool{
  margin-bottom: 10px;
  .btn+.btn{
    margin-left:30px;
  }
}
.fixed-table-loading{
  line-height: 40px;
}

//弹框
.modal{
  .bootstrap-table,.table{
    min-width: 100%;
  }
  .modal-header{
    padding:12px 20px;
    background:@color-body-bg;
    border-bottom:1px solid #eaeaea;
  }
  .modal-title{
    font-size:14px;
    color:#000
  }
  .nav-tabs-custom .nav-tabs > li a{
    font-size:14px;
  }
  .modal-footer{
    padding-left:174px;
    text-align: left;
    .btn+.btn{
      margin-left:30px;
    }
  }
  .modal-dialog .modal-content{
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    overflow: hidden;
  }
  .input-group>.label-head{
    color:#777;
  }
}
//分页
.fixed-table-pagination{
  .pagination>.active>a, .pagination>.active>a:focus, .pagination>.active>a:hover, .pagination>.active>span, .pagination>.active>span:focus, .pagination>.active>span:hover{
    background-color: @color-blue;
    border-color: #1262b7;
  }
  .dropdown-menu>.active>a, .dropdown-menu>.active>a:focus, .dropdown-menu>.active>a:hover{
    background-color:@color-blue
  }
}
.btn-group>.btn-white:first-child:not(:last-child):not(.dropdown-toggle){
  border-radius: 3px;
}
//按钮
.btn-white{
  background: #fff;
  border-radius: 3px;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-box-shadow: 0;
  -moz-box-shadow: 0;
  min-width: 80px;
  padding:4px 10px;
  &:hover{
    background-color: @color-blue !important;
    border: 1px solid @color-blue !important;
    color: #fff !important;
  }
}
.btn-primary{
  background: @color-blue;
  border: 1px solid transparent;
  vertical-align: middle;
  span{
    display: inline-block;
    vertical-align: middle;
  }
  &:hover,&:active{
    background: @color-hover-blue;
    border:1px solid @color-hover-blue;
    color: #fff;
  }
}
.btn-primary-lg{
  padding:5px 20px;
}
.btn-primary-sm{
  padding:2px 15px;
  font-size:12px;
}
.btn-cancel{
  background: #dce5ff;
  color:@color-blue;
  border: transparent;
  &:hover,&:active{
    background: #d8deed;
    border:1px solid transparent;
    color: #4e7fff;
  }
}
.btn-add{
  border:1px solid @color-border;
  color:@color-blue;
  padding:4px 16px;
  font-size:14px;
  &:hover,&:active{
    border:1px solid @color-blue;
    color:@color-blue;
  }
}
.btn-a{
  display: inline-block;
  cursor: pointer;
  &:hover{
    color:@color-blue;
  }
}
.btn-upload{
  color:@color-blue;
  border:1px solid @color-blue;
  margin-left:5px;
  &:hover,&:active{
    color:#fff;
    background: @color-blue;
  }
}
.btn-group-edit{
  .btn{
    margin:5px 2px;
  }
}

//label 长度auto input etc
.form-label-auto{
  .input-group {
    display: inline-table !important;
    vertical-align: middle;
    border-collapse: separate;
    position: relative;
    width: 100%;
  }
  .label-head{
    display: table-cell;
    width: 80px;
    vertical-align: middle;
    &>span{
      display: inline-block;
      position: relative;
    }
    &.text-right{
      padding-right:10px;
    }
  }
  .label-unit{
    width: 30px;
    height:100%;
    text-align: center;
    padding-top:7px;
    float: left;
  }
  .label-box .label-unit{
    position: absolute;
  }
  .label-head-lg{
    width: 130px;
  }
  .label-head-llg{
    width: 135px;
  }
  .label-name{
    color:#333;
  }
  .label-head-top{
    vertical-align: top;
    padding-top: 7px;
    &.require:after{
      top:7px;
      bottom: auto;
    }
  }
  .form-control,.label-box{
    display: table-cell;
    width: 100%;
    vertical-align: middle;
    float: left;
    position: relative;
    .label-box-item{
      vertical-align: middle;
      min-width: 160px;
      cursor: pointer;
      margin:0;
      input{
        margin-right: 10px;
        vertical-align: top;
      }
    }
    .label-error{
      padding:6px;
      display: inline-block;
      vertical-align: middle;
    }
  }
  .label-box-sm{
    width: 200px;
  }
  .label-head .label-box-lg{
    width:320px;
  }
  .label-box+.label-box{
    margin-left:10px;
  }

}
@media (max-width: 1198px) {
  .form-label-auto{
    .label-box+.label-box{
      margin-left:0;
    }
  }

}
.label-box-top{
