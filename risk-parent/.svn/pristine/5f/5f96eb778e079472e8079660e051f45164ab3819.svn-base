//申请表单
.pages-form{
  .inline-block{
    display: inline-block;
    vertical-align: middle;
  }
  .require{
    position: relative;
    padding-left:14px;
  }
  .require:after{
    content:'*';
    color:red;
    font-size:18px;
    height:18px;
    position: absolute;
    top:0;
    bottom:0;
    margin:auto;
    left:0;
    vertical-align: middle;
  }
  .input-group-addon .require:after{
    font-size:18px !important;
    height:12px;
  }
  .input-group-addon-small {
    width: 120px !important;
  }

  .input-group-addon {
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
  }

  .city-picker-span {
    width: 100% !important;
    border: 1px solid #d4dadc;
    padding-left:10px;
  }
  .content-wrapper {
    height: auto;
  }
  .input-group-btn > .btn + .btn {
    margin-left: -1px !important;
  }
  .input-group-btn {
    width: 1% !important;
    white-space: nowrap;
    vertical-align: middle;
  }

  .file-caption-name {
    vertical-align: middle;
    display: inline-block;
    overflow: hidden;
    word-break: break-all;
    height: 20px;
  }

  .file-caption-name i {
    top: 2px;
  }

  .webuploader-container {
    display: inline-block;
  }

  .webuploader-pick {
    background: none;
    padding: 0;
    color: inherit;
    border-radius: 0;
    display: block;
  }

  .webuploader-pick span {
    display: inline-block;
    margin-left: 5px;
  }

  .uploadError {
    margin-left: 15px;
  }
  .filelist{
    margin-top:10px;
  }
  .filelist li{
    list-style: none;
    margin-bottom:5px;
  }
  .fileNote{
    width: 20px;
    height: 20px;
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
  }
  .fileSuccess{
    color:#5cb85c;
  }
  .fileError{
    color:#d9534f;
  }
  .checkbox-inline+.checkbox-inline, .radio-inline+.radio-inline{
    margin-left:0;
  }
  .checkbox-inline,.radio-inline{
    margin-right:10px;
    padding-top:7px;
  }
  .relative{
    position: relative;
  }
  .input-unit{
    position: absolute;
    top:0;
    bottom: 0;
    right:0;
    margin:auto;
    height:100%;
    padding: 0 12px;
    font-size: 14px;
    font-weight: 400;
    line-height: 1;
    color: #555;
    background: #fff;
    text-align: center;
    border: 1px solid #ccc;
    z-index: 10;
  }
  .input-unit i{
    vertical-align: middle;
  }
  .input-unit:after{
    content: '';
    display: inline-block;
    width: 0;
    height:100%;
    vertical-align: middle;
  }
  .input-group{
    width: 100%;
  }
  .relative.city-picker-input{
    top:0;
    left:0
  }

  /*menu*/
  .float_menu{
    position: fixed;
    top:120px;
    right:0;
    z-index: 9999;
    font-size:0;
    width: 150px;

    color:#afb5c4;
  }
  .float_menu li{
    height:30px;
    text-align: center;
    line-height: 30px;
    list-style: none;
    cursor: pointer;
    padding:0 10px;
    vertical-align: middle;
    color:#afb5c4;
    &:hover{
      color:@color-blue
    }
  }
  .float_menu li a,.float_menu li a:hover,.float_menu li a:active{
    color:#afb5c4;
  }
  .float_menu li:hover,.float_menu li:hover a,.menu_arrow:hover{
    background: #000 !important;
    color:#4e73d9;
  }
  .float_menu ul{
    padding:0;
    margin:0;
  }
  .float_menu .menu_content{
    max-height: 80%;
    overflow: auto;
    display: inline-block;
    vertical-align: middle;
    border: 1px solid #d4d4d4;
    background: #4d4d4f;
    font-size:12px;
    width: 120px;
    white-space: nowrap;
  }
  .float_menu .menu_content .menu_li{
