<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.sunfit.risk.credit.server.dao.ApiLogDAO" >
  <resultMap id="BaseResultMap" type="cn.sunfit.risk.credit.api.beans.ApiLog" >
    <id column="id" property="id" jdbcType="VARCHAR" />
    <result column="ip_address" property="ipAddress" jdbcType="VARCHAR" />
    <result column="uid" property="uid" jdbcType="VARCHAR" />
    <result column="api_platform" property="apiPlatform" jdbcType="VARCHAR" />
    <result column="api_name" property="apiName" jdbcType="VARCHAR" />
    <result column="req_time" property="reqTime" jdbcType="TIMESTAMP" />
    <result column="req" property="req" jdbcType="LONGVARCHAR" />
    <result column="resp" property="resp" jdbcType="LONGVARCHAR" />
    <result column="cached" property="cached" jdbcType="TINYINT" />
  </resultMap>
  
  <sql id="Base_Column_List" >
    id, ip_address, uid, api_platform, api_name, req_time, req, resp, cached
  </sql>
  
  <insert id="insert" parameterType="cn.sunfit.risk.credit.api.beans.ApiLog" >
    insert into risk_api_log (id, ip_address, uid, 
      api_platform, api_name, req_time, cached,
      req, resp)
    values (#{id,jdbcType=VARCHAR}, #{ipAddress,jdbcType=VARCHAR}, #{uid,jdbcType=VARCHAR}, 
      #{apiPlatform,jdbcType=VARCHAR}, #{apiName,jdbcType=VARCHAR}, #{reqTime,jdbcType=TIMESTAMP}, 
      #{cached,jdbcType=TINYINT}, #{req,jdbcType=LONGVARCHAR}, #{resp,jdbcType=LONGVARCHAR})
  </insert>
  
  <select id="selectByReq" resultType="cn.sunfit.risk.credit.api.beans.ApiLog" resultMap="BaseResultMap">
  	select <include refid="Base_Column_List"/> from risk_api_log 
  	where api_platform = #{apiPlatform,jdbcType=VARCHAR}
  	and api_name = #{apiName,jdbcType=VARCHAR}
  	and req = #{req,jdbcType=LONGVARCHAR}
  </select>
</mapper>