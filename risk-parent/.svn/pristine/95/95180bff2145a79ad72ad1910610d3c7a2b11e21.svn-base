package cn.sunfit.risk.buz.api.constants.asset;

public enum ZnjCalType {
    // 没有滞纳金
    NO_ZNJFEE("0", "否"),
    // 有滞纳金
    HAS_ZNJFEE("1", "是");

    public static ZnjCalType getTypeNameByTypeId(String typeId) {
        for (ZnjCalType assetType : ZnjCalType.values()) {
            if (assetType.typeId.equals(typeId)) {
                return assetType;
            }
        }
        return null;
    }

    public static String getTypeNameStrByTypeId(String typeId) {
        for (ZnjCalType assetType : ZnjCalType.values()) {
            if (assetType.typeId.equals(typeId)) {
                return assetType.getTypeName();
            }
        }
        return null;
    }

    private final String typeId;

    private final String typeName;

    ZnjCalType(String typeId, String typeName) {
        this.typeId = typeId;
        this.typeName = typeName;
    }

    public String getTypeId() {
        return typeId;
    }

    public String getTypeName() {
        return typeName;
    }

}
