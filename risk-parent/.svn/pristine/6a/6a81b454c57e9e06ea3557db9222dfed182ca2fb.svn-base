  left: 0;
}
.custom-form .float_menu {
  position: fixed;
  top: 2px;
  right: 10px;
  z-index: 9999;
  height: 100%;
  font-size: 0;
  width: 150px;
}
.custom-form .float_menu li {
  height: 30px;
  text-align: center;
  line-height: 30px;
  list-style: none;
  cursor: pointer;
  padding: 0 10px;
  vertical-align: middle;
}
.custom-form .float_menu li a,
.custom-form .float_menu li a:hover,
.custom-form .float_menu li a:active {
