package cn.sunfit.risk.buz.api.vo.solution.dyc;

import java.io.Serializable;
import java.util.List;

public class DycFeeConfig implements Serializable {

    private static final long serialVersionUID = 1L;
    // 保证金费率
    private List<Double> bzjFee;
    // 保证金费率
    private List<Double> wyFee;
    // 滞纳金
    private List<Double> znjFee;
    // 每期时长
    private List<EachTime> eachTimes;
    // 固定费率 1固定综合费率 2固定管理费
    private String feeCaType;
    // 费用关联方
    private List<FeeRelated> feeRelated;
    // GPS费用
    private List<Double> gpsFee;
    // GPS服务费
    private List<Double> gpsSerFee;
    // 月标配置
    private List<MonthlyFee> monthlyFee;
    // 日标配置
    private List<DaylyFee> daylyFee;
    // 月综合费率
    private List<MonthlyZHFee> monthlyZHFee;
    // 月管理费
    private List<Double> monthlyGLFee;
    // 日综合费率
    private List<MonthlyZHFee> daylyZHFee;
    // 日管理费
    private List<Double> daylyGLFee;
    // 还款方式
    private List<RepaymentType> repaymentTypes;
    // 滞纳金计算方式 1 剩余本金*逾期天数*滞纳金率 2合同金额*逾期天数*滞纳金率
    private String znjFeeCal;
    // 年月日率计算方式设置 1年化率=日利率*365，月利率=年化率/12 2年化率=日利率*360，月利率=年化率/12
    private String monthlyFeeCal;

    public List<Double> getBzjFee() {
        return bzjFee;
    }

    public List<DaylyFee> getDaylyFee() {
        return daylyFee;
    }

    public List<Double> getDaylyGLFee() {
        return daylyGLFee;
    }

    public List<MonthlyZHFee> getDaylyZHFee() {
        return daylyZHFee;
    }

    public List<EachTime> getEachTimes() {
        return eachTimes;
    }

    public String getFeeCaType() {
        return feeCaType;
    }

    public List<FeeRelated> getFeeRelated() {
        return feeRelated;
    }

    public List<Double> getGpsFee() {
        return gpsFee;
    }

    public List<Double> getGpsSerFee() {
        return gpsSerFee;
    }

    public List<MonthlyFee> getMonthlyFee() {
        return monthlyFee;
    }

    public String getMonthlyFeeCal() {
        return monthlyFeeCal;
    }

    public List<Double> getMonthlyGLFee() {
        return monthlyGLFee;
    }

    public List<MonthlyZHFee> getMonthlyZHFee() {
        return monthlyZHFee;
    }

    public List<RepaymentType> getRepaymentTypes() {
        return repaymentTypes;
    }

    public List<Double> getWyFee() {
        return wyFee;
    }

    public List<Double> getZnjFee() {
        return znjFee;
    }

    public String getZnjFeeCal() {
        return znjFeeCal;
    }

    public void setBzjFee(List<Double> bzjFee) {
        this.bzjFee = bzjFee;
    }

    public void setDaylyFee(List<DaylyFee> daylyFee) {
        this.daylyFee = daylyFee;
    }

    public void setDaylyGLFee(List<Double> daylyGLFee) {
        this.daylyGLFee = daylyGLFee;
    }

    public void setDaylyZHFee(List<MonthlyZHFee> daylyZHFee) {
        this.daylyZHFee = daylyZHFee;
    }

    public void setEachTimes(List<EachTime> eachTimes) {
        this.eachTimes = eachTimes;
    }

    public void setFeeCaType(String feeCaType) {
        this.feeCaType = feeCaType;
    }

    public void setFeeRelated(List<FeeRelated> feeRelated) {
        this.feeRelated = feeRelated;
    }

    public void setGpsFee(List<Double> gpsFee) {
        this.gpsFee = gpsFee;
    }

    public void setGpsSerFee(List<Double> gpsSerFee) {
        this.gpsSerFee = gpsSerFee;
    }

    public void setMonthlyFee(List<MonthlyFee> monthlyFee) {
        this.monthlyFee = monthlyFee;
    }

    public void setMonthlyFeeCal(String monthlyFeeCal) {
        this.monthlyFeeCal = monthlyFeeCal;
    }

    public void setMonthlyGLFee(List<Double> monthlyGLFee) {
        this.monthlyGLFee = monthlyGLFee;
    }

    public void setMonthlyZHFee(List<MonthlyZHFee> monthlyZHFee) {
        this.monthlyZHFee = monthlyZHFee;
    }

