package cn.sunfit.risk.buz.api.vo.solution;

import cn.sunfit.risk.buz.api.vo.CorpReqBase;

/**
 * 
 * @Title: ProductQueryReq.java
 * @Package cn.sunfit.risk.buz.api.vo.solution
 * @Description: 查询公司产品的流程列表参数
 * @author XFL
 * @date 2016年12月29日 下午4:08:53
 * @version V1.0
 */
public class BpMetaQueryReq extends CorpReqBase {

    private static final long serialVersionUID = 8355948445871981535L;

    private String productId;

    private boolean lastest;

    public BpMetaQueryReq() {
        super();
    }

    public BpMetaQueryReq(String productId, boolean lastest, String domain) {
        super();
        this.productId = productId;
        this.lastest = lastest;
        setDomain(domain);
    }

    public String getProductId() {
        return productId;
    }

    public boolean isLastest() {
        return lastest;
    }

    public void setLastest(boolean lastest) {
        this.lastest = lastest;
    }

    public void setProductId(String productId) {
        this.productId = productId;
    }

}
