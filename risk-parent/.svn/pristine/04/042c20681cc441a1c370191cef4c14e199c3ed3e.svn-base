<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.sunfit.risk.buz.server.dao.p2p.excel.OperationLogDAO" >
  <resultMap id="BaseResultMap" type="cn.sunfit.risk.buz.api.beans.p2p.P2PCustOperLog" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="uid" property="uid" jdbcType="VARCHAR" />
    <result column="orderId" property="order_id" jdbcType="BIGINT" />
    <result column="operType" property="oper_type" jdbcType="VARCHAR" />
    <result column="detail" property="detail" jdbcType="VARCHAR" />
    <result column="operTime" property="oper_time" jdbcType="TIMESTAMP" />
  </resultMap>
	
  <insert id="insertOperLog" parameterType="cn.sunfit.risk.buz.api.beans.p2p.P2PCustOperLog">
  	INSERT INTO p2p_cust_oper_log(uid,order_id,oper_type,detail,oper_time)
  		VALUES(#{uid,jdbcType=VARCHAR},#{orderId,jdbcType=BIGINT},#{operType,jdbcType=VARCHAR},#{detail,jdbcType=VARCHAR},
  		#{operTime,jdbcType=TIMESTAMP})
  </insert>
</mapper>