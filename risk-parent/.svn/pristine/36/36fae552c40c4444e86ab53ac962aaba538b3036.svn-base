<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.sunfit.risk.buz.server.dao.bank.BankDAO" >
  <resultMap id="BaseResultMap" type="cn.sunfit.risk.buz.api.beans.bank.DmBank" >
    <id column="id" property="id" jdbcType="VARCHAR" />
    <result column="bank_code" property="bankCode" jdbcType="VARCHAR" />
    <result column="bank_name" property="bankName" jdbcType="VARCHAR" />
    <result column="bank_short" property="bankShort" jdbcType="VARCHAR" />
    <result column="third_type" property="thirdType" jdbcType="VARCHAR" />
    <result column="effect_tag" property="effectTag" jdbcType="SMALLINT" />
  </resultMap>
  <sql id="Base_Column_List" >
    id,bank_code,bank_name,bank_short,third_type,effect_tag
  </sql>
  
  <select id="findAll" resultMap="BaseResultMap">
  	select <include refid="Base_Column_List"/>
  	from p2p_bank 	
  </select>
  
</mapper>