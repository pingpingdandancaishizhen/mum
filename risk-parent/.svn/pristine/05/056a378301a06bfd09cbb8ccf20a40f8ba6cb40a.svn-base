package cn.sunfit.risk.buz.api.vo.corp;

import cn.sunfit.risk.buz.api.beans.corp.Article;

public class ArticleQueryDTO extends Article {

    private static final long serialVersionUID = 1L;

    private String typeName;

    private String statusName;

    private String artWriterName;

    private String artReviewerName;

    public String getTypeName() {
        return typeName;
    }

    public void setTypeName(String typeName) {
        this.typeName = typeName;
    }

    public String getStatusName() {
        return statusName;
    }

    public void setStatusName(String statusName) {
        this.statusName = statusName;
    }

    public String getArtWriterName() {
        return artWriterName;
    }

    public void setArtWriterName(String artWriterName) {
        this.artWriterName = artWriterName;
    }

    public String getArtReviewerName() {
        return artReviewerName;
    }

    public void setArtReviewerName(String artReviewerName) {
        this.artReviewerName = artReviewerName;
    }

}
