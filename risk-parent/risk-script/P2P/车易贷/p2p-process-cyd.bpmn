  <process id="p2p-process-cyd" name="p2p-process-cyd" isExecutable="true">
    <startEvent id="p2p_start" name="开始"></startEvent>
    <endEvent id="p2p_wait_fk" name="待放款"></endEvent>
    <userTask id="p2p_add_order" name="新增订单"></userTask>
    <userTask id="p2p_order_audit" name="P2P审核"></userTask>
    <userTask id="p2p_image_audit" name="P2P影像审核"></userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway" default="flow2"></exclusiveGateway>
    <sequenceFlow id="flow1" sourceRef="p2p_start" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow2" name="保存" sourceRef="exclusivegateway1" targetRef="p2p_add_order"></sequenceFlow>
    <sequenceFlow id="flow3" name="特定产品导入直接到P2P审核" sourceRef="exclusivegateway1" targetRef="p2p_order_audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '1' && to_node=='1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" name="特定产品导入直接到P2P影像审核" sourceRef="exclusivegateway1" targetRef="p2p_image_audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '1' && to_node=='1,2'}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway" default="flow6"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="p2p_add_order" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow6" name="通过" sourceRef="exclusivegateway2" targetRef="p2p_order_audit"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway" default="flow8"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="p2p_order_audit" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow8" name="通过" sourceRef="exclusivegateway3" targetRef="p2p_image_audit"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow9" sourceRef="p2p_image_audit" targetRef="exclusivegateway4"></sequenceFlow>
    <sequenceFlow id="flow10" name="通过" sourceRef="exclusivegateway4" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '1'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="p2p_cancle" name="作废"></endEvent>
    <sequenceFlow id="flow11" name="作废" sourceRef="exclusivegateway2" targetRef="p2p_cancle">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '2'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" name="退回" sourceRef="exclusivegateway4" targetRef="p2p_back_order">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '-1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" name="退回" sourceRef="exclusivegateway3" targetRef="p2p_back_order">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '-1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="exclusivegateway1" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '1' && to_node=='1,2,3'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="exclusivegateway2" targetRef="p2p_image_audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '1' && to_node=='2'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow16" sourceRef="exclusivegateway2" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '1' && to_node=='2,3'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="exclusivegateway3" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${oper_type == '1' && to_node=='3'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask1" name="P2P发标" activiti:delegateExpression="${p2pPushTask}"></serviceTask>
    <sequenceFlow id="flow18" sourceRef="servicetask1" targetRef="wait_p2p_audit"></sequenceFlow>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition errorRef="P2PPUSHERROR"></errorEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow19" name="调用接口异常" sourceRef="boundaryerror1" targetRef="p2p_image_audit"></sequenceFlow>
    <userTask id="p2p_back_order" name="退回订单"></userTask>
    <sequenceFlow id="flow20" sourceRef="p2p_back_order" targetRef="exclusivegateway2"></sequenceFlow>
    <receiveTask id="wait_p2p_audit" name="等待P2P初审"></receiveTask>
    <exclusiveGateway id="exclusivegateway5" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow21" sourceRef="wait_p2p_audit" targetRef="exclusivegateway5"></sequenceFlow>
    <sequenceFlow id="flow22" name="P2P初审被拒绝" sourceRef="exclusivegateway5" targetRef="p2p_image_audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${P2P_RESULT == '2'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow23" name="P2P初审通过" sourceRef="exclusivegateway5" targetRef="p2p_wait_fk">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${P2P_RESULT == '1'}]]></conditionExpression>
    </sequenceFlow>
  </process>
