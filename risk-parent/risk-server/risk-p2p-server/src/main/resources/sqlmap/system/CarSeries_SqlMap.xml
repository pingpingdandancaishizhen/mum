<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.sunfit.risk.buz.server.dao.system.CarSeriesDAO" >
  <resultMap id="BaseResultMap" type="cn.sunfit.risk.buz.api.beans.system.CarSeries" >
    <result column="series_id" property="seriesId" jdbcType="INTEGER" />
    <result column="brand_id" property="brandId" jdbcType="INTEGER" />
    <result column="series_name" property="seriesName" jdbcType="VARCHAR" />
    <result column="show_order" property="showOrder" jdbcType="INTEGER" />
  </resultMap>
  
  <select id="selectAllCarSeries" resultMap="BaseResultMap">
  	select series_id, brand_id, series_name, 
      show_order from 
      risk_car_series 
      order by brand_id ASC
  </select>
</mapper>